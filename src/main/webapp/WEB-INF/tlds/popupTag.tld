    <tag>
        <name>cust</name>
        <tag-class>ever.support.taglibs.PopupCustInfoTag</tag-class>
        <attribute>
            <name>pType</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <description>팝업종류를 선택하세요.</description>
        </attribute>
        <attribute>
            <name>pCode</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <description>* 코드ID를 입력하세요.</description>
        </attribute>
        <attribute>
            <name>pName</name>
